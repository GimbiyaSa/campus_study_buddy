{
  "name": "study-buddy-api",
  "version": "1.0.0",
  "description": "Study Buddy Platform API",
  "type": "commonjs",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon dist/app.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "config:health": "node dist/config/healthCheckTest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "seed:test-user": "node src/database/seed_test_user.js",
    "worker:notifications": "node workers/notificationsWorker.js"
  },
  "dependencies": {
    "@azure/identity": "^4.12.0",
    "@azure/keyvault-secrets": "^4.10.0",
    "@azure/service-bus": "^7.9.0",
    "@azure/storage-blob": "^12.28.0",
    "@azure/web-pubsub": "^1.2.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "google-auth-library": "^10.3.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.0.1",
    "mssql": "^12.0.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/mssql": "^9.1.8",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
